

a = input("Вводи первую строку для сравнения множеств: ")
b = input("Вводи вторую строку для сравнения множеств: ")



set_a = set(a)
set_b = set(b)

list_a = list(a)
list_b = list(b)


print('Вторая строка для сравнения: ', a)
print('Вторая строка для сравнения: ', b)

print('первый список: ', list_a)
print('второй список: ', list_b)

print('первое множество: ', set_a)
print('второе множество: ', set_b)


if set_a == set_b:
    print('Множества элементов совпадают!')
else:
    print('Множества элементов не совпадают!')
